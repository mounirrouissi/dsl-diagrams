START_TRACK AN_CHEVY_GM_ALERT_NRTA_1ST_TRACK
	S1. START
		SENDMESSAGE SMS AGENT CUST $GENERIC_GM_ALERT_NRTA_1ST_TEMP
        MARK_LEAD_PHASE CONTACTED
        SCHEDULE_FOLLOWUP RELDAY:1 ABSTIME:09:12:am GENERIC_GM_ALERT_NRTA_1ST_NRAA_TRACK:S1
	    END
	S1-R. START
		    UNSCHEDULE CUST
		    UNSCHEDULE SREP
		    SWITCH_DIRECTION OUTBOUND
