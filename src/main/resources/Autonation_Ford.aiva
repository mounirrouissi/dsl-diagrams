START_TRACK FORD_5K_NEW_NRTA_1ST_TRACK
	S1. START
		    SENDMESSAGE SMS AGENT CUST $AN_FORD_5K_NEW_NRTA_1ST_TEMP
            MARK_LEAD_PHASE CONTACTED
		    SCHEDULE_FOLLOWUP RELDAY:1 ABSTIME:09:57:am GENERIC_AN_FIRSTSERVICE_NRTA_1ST_NRAA_TRACK_V1:S1
	    END
	S1-R. START
		    UNSCHEDULE CUST
		    UNSCHEDULE SREP
		    SWITCH_DIRECTION OUTBOUND
            MARK_LEAD_PHASE RESPONDED
		    GOTO GENERIC_NRTA_1STTEXT_TRACK_RESPONSE_HANDLER:S1
	      END
