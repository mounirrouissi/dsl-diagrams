	S1. START
        CALL_FUNCTION_SWITCH jaguarUtils checkServicePackage -> @servicePackage
            R1. TOYO_5K_NEW > SENDMESSAGE_INTERNAL SMS AGENT SREP $AN_TOYOTA_5K_NEW_NRTA_2ND_TEMP
            R2. TOYO_10K_NEW > SENDMESSAGE_INTERNAL SMS AGENT SREP $AN_TOYOTA_10K_NRTA_2ND_TEMP
            R3. TOYO_15K_NEW > SENDMESSAGE_INTERNAL SMS AGENT SREP $AN_TOYOTA_15K_NEW_NRTA_2ND_TEMP
            R4. TOYO_20K_NEW > SENDMESSAGE_INTERNAL SMS AGENT SREP $AN_TOYOTA_20K_NEW_NRTA_2ND_TEMP
            R5. TOYO_25K_NEW > SENDMESSAGE_INTERNAL SMS AGENT SREP $AN_TOYOTA_25K_NEW_NRTA_2ND_TEMP
            R6. TOYO_5K_USED > SENDMESSAGE_INTERNAL SMS AGENT SREP $GENERIC_AN_5K_USED_NRTA_2ND_TEMP
            R7. TOYO_NS1 > SENDMESSAGE_INTERNAL SMS AGENT SREP $GENERIC_AN_NEXTSERVICE_NRTA_2ND_TEMP
            R8. TOYO_NS2 > SENDMESSAGE_INTERNAL SMS AGENT SREP $GENERIC_AN_NEXTSERVICE_NRTA_2ND_TEMP
            R9. TEA > SENDMESSAGE_INTERNAL SMS AGENT SREP $NRTA_CUSTOMER_A_TEA_NEXT
            R_default. GOTO MANUAL_INTERNAL_HANDOFF:S1
	    END
	S1-R. START
		    UNSCHEDULE CUST
		    UNSCHEDULE SREP
            MARK_LEAD_PHASE ENGAGED
            GOTO GENERIC_ASK_4APPT_TRACK:S1
	      END
END_TRACK

