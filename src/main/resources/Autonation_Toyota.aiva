START_TRACK GENERIC_CUST_SPECIFY_MILEAGE_HANDLER
	S1.  START
		    CALL_FUNCTION_SWITCH jaguarUtils checkServicePackage -> @servicePackage
			    //R1. TOYO_NS1 > (Need to define action)
			    R_default. GOTO GENERIC_CUST_SPECIFY_MILEAGE_HANDLER:S2
	     END
    S1-R. START
            UNSCHEDULE CUST
            UNSCHEDULE SREP
            SWITCH_DIRECTION OUTBOUND
    	    GOTO GENERIC_ASK_4APPT_TRACK:S1
          END
	S2.  START
		    CALL_FUNCTION_SWITCH jaguarUtils getMileageStatusByService -> @mileageStatus
		        R1. ALMOST_LIMIT_25K > SENDMESSAGE_INTERNAL SMS AGENT SREP $BASED_ON_MILEAGE_V2_25K
            	R1. ABOVE_LIMIT_25K > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_EXCEEDED_TEMP
            	R1. UNDER_LIMIT_25K > SENDMESSAGE_INTERNAL SMS AGENT SREP $JAGUAR_OILCHANGE_CUSTOMER_A_LOW_MILEAGE_25K
            	R1. EXACT_LIMIT > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_ARE_EXACT_TEMP

			    R1. ALMOST_LIMIT_20K > SENDMESSAGE_INTERNAL SMS AGENT SREP $BASED_ON_MILEAGE_V2_20K
			    R1. ABOVE_LIMIT_20K > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_EXCEEDED_TEMP
			    R1. UNDER_LIMIT_20K > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_CUST_UNDER_LIMIT_20K_TEMP
			    R1. EXACT_LIMIT > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_ARE_EXACT_TEMP

			    R1. ALMOST_LIMIT_15K > SENDMESSAGE_INTERNAL SMS AGENT SREP $BASED_ON_MILEAGE_V2_15K
			    R1. ABOVE_LIMIT_15K > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_EXCEEDED_TEMP
			    R1. UNDER_LIMIT_15K > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_CUST_UNDER_LIMIT_15K_TEMP
			    R1. EXACT_LIMIT > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_ARE_EXACT_TEMP

			    R1. ALMOST_LIMIT_10K > SENDMESSAGE_INTERNAL SMS AGENT SREP $JAGUAR_OILCHANGE_CUSTOMER_B2_OVERRESTIMATED_MILEAGE
			    R1. ABOVE_LIMIT_10K > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_EXCEEDED_TEMP
			    R1. UNDER_LIMIT_10K > SENDMESSAGE_INTERNAL SMS AGENT SREP $AN_TOYOTA_OBJECTION_LOW_MILEAGE_10K_TEMP
			    R1. EXACT_LIMIT > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_ARE_EXACT_TEMP

                R1. ALMOST_LIMIT > SENDMESSAGE_INTERNAL SMS AGENT SREP $JAGUAR_OILCHANGE_CUSTOMER_A_SPECIFY_MILEAGE_ALMOST_LIMIT
			    R1. ABOVE_LIMIT > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_EXCEEDED_TEMP
			    R1. UNDER_LIMIT > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_OBJECTION_LOW_MILEAGE_V2_5K_TEMP
			    R1. EXACT_LIMIT > SENDMESSAGE_INTERNAL SMS AGENT SREP $AUTONATION_WHEN_MILEAGE_ARE_EXACT_TEMP
			    R_default. GOTO MANUAL_INTERNAL_HANDOFF:S1
	     END
     S2-R. START
             UNSCHEDULE CUST
             UNSCHEDULE SREP
             SWITCH_DIRECTION OUTBOUND
     	     GOTO GENERIC_ASK_4APPT_TRACK:S1
           END
END_TRACK

