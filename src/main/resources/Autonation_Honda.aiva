START_TRACK GENERIC_NRTA_2NDTEXT_TRACK
	S1. START
        CALL_FUNCTION_SWITCH jaguarUtils checkServicePackage -> @servicePackage
            R1. HND_90D > SENDMESSAGE_INTERNAL SMS AGENT SREP $AN_HONDA_90D_NEW_NRTA_2ND_TEMP
            R2. HND_7500_NEW > SENDMESSAGE_INTERNAL SMS AGENT SREP $AN_HONDA_7500_NEW_NRTA_2ND_TEMP
            R3. HND_15K_NEW > SENDMESSAGE_INTERNAL SMS AGENT SREP $AN_HONDA_15K_NRTA_2ND_TEMP
            R4. HND_5K_USED > SENDMESSAGE_INTERNAL SMS AGENT SREP $GENERIC_AN_5K_USED_NRTA_2ND_TEMP
            R5. HND_NS1 > SENDMESSAGE_INTERNAL SMS AGENT SREP $GENERIC_AN_NEXTSERVICE_NRTA_2ND_TEMP
            R6. HND_NS2 > SENDMESSAGE_INTERNAL SMS AGENT SREP $GENERIC_AN_NEXTSERVICE_NRTA_2ND_TEMP
            R_default. GOTO MANUAL_INTERNAL_HANDOFF:S1
	    END
	S1-R. START
		    UNSCHEDULE CUST
		    UNSCHEDULE SREP
            MARK_LEAD_PHASE ENGAGED
            GOTO GENERIC_ASK_4APPT_TRACK:S1
	      END
END_TRACK

