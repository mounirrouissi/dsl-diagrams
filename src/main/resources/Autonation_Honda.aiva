//This is the very first default track that called from AIVA file when we initiate the lead
START_TRACK DEFAULT

    S1. START

        DELETE_INSIGHT CL
        SETMARK FOLLOW_UP FALSE
        SETMARK CONTACT_LATER FALSE

		CALL_FUNCTION_SWITCH customerUtils findCustomerType -> @customerType

			R1. HONDA_90D_NEW_A_NRTA > GOTO AN_HONDA_90D_NEW_NRTA_1ST_TRACK:S1
			R1. HONDA_90D_NEW_A_NRTA_TIME > GOTO AN_HONDA_90D_NEW_NRTA_1ST_TRACK:S1
			R1. HONDA_90D_NEW_A_RTA > GOTO AN_HONDA_90D_NEW_RTA_1ST_TRACK:S1
