//-------------------------------------------- (7.5K NEW) (Type NS/OS) NRTA/RTA Track Starting-------------------------------------//

// -------------------------------------NRTA

START_TRACK AN_HONDA_7500_NEW_NRTA_1ST_TRACK
	S1. START
	    SENDMESSAGE SMS AGENT CUST $AN_HONDA_7500_NEW_NRTA_1ST_TEMP
        MARK_LEAD_PHASE CONTACTED
        SCHEDULE_FOLLOWUP RELDAY:1 ABSTIME:09:35:am GENERIC_AN_FIRSTSERVICE_NRTA_1ST_NRAA_TRACK_V1:S1
       END
	S1-R. START
	    UNSCHEDULE CUST
	    UNSCHEDULE SREP
	    SWITCH_DIRECTION OUTBOUND
        MARK_LEAD_PHASE RESPONDED
	    GOTO GENERIC_NRTA_1STTEXT_TRACK_RESPONSE_HANDLER:S1
	END
END_TRACK

START_TRACK AN_HONDA_7500_NEW_NRTA_1ST_NRAA_TRACK
	S1. START
		IS_SUNDAY -> @isSunday
			 R1. true > SCHEDULE_FOLLOWUP RELDAY:1 ABSTIME:09:35:am AN_HONDA_7500_NEW_NRTA_1ST_NRAA_TRACK:S1
			 R1. false > SENDMESSAGE SMS AGENT CUST $AN_HONDA_7500_NEW_NRTA_1ST_NRAA_TEMP;SETMARK NO_RESPONSE_AT_ALL TRUE;SCHEDULE_FOLLOWUP RELDAY:4 ABSTIME:09:35:am AN_HONDA_7500_NEW_NRTA_2ND_NRAA_TRACK:S1
			 R_default. SENDMESSAGE SMS AGENT CUST $AN_HONDA_7500_NEW_NRTA_1ST_NRAA_TEMP;SETMARK NO_RESPONSE_AT_ALL TRUE;SCHEDULE_FOLLOWUP RELDAY:4 ABSTIME:09:35:am AN_HONDA_7500_NEW_NRTA_2ND_NRAA_TRACK:S1
	    END
    S1-R. START
	    UNSCHEDULE CUST
	    UNSCHEDULE SREP
	    SWITCH_DIRECTION OUTBOUND
	    GOTO GENERIC_NRTA_1STTEXT_TRACK_RESPONSE_HANDLER:S1
      END
END_TRACK

START_TRACK AN_HONDA_7500_NEW_NRTA_2ND_NRAA_TRACK
	S1. START
		IS_SUNDAY -> @isSunday
			 R1. true > SCHEDULE_FOLLOWUP RELDAY:1 ABSTIME:09:35:am AN_HONDA_7500_NEW_NRTA_2ND_NRAA_TRACK:S1
			 R1. false > SENDMESSAGE SMS AGENT CUST $AN_HONDA_7500_NEW_NRTA_2ND_NRAA_TEMP;SETMARK NO_RESPONSE_AT_ALL TRUE
			 R_default. SENDMESSAGE SMS AGENT CUST $AN_HONDA_7500_NEW_NRTA_2ND_NRAA_TEMP;SETMARK NO_RESPONSE_AT_ALL TRUE
	END
	S1-R. START
